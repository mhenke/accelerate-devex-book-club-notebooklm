---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/feeds/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/podcast.xml</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/videos.xml</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/media-feed.xml</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  
  {% for page in site.pages %}
    {% if page.layout and page.permalink %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ page.url }}</loc>
      <lastmod>{{ page.date | default: site.time | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.7</priority>
    </url>
    {% endif %}
  {% endfor %}
  
  {% for meeting in site.meetings %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ meeting.url }}</loc>
      <lastmod>{{ meeting.date | default: site.time | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.8</priority>
    </url>
  {% endfor %}
</urlset>
