---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" 
     xmlns:media="http://search.yahoo.com/mrss/"
     xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title }} - Video Series</title>
    <description>{{ site.description }} - Video content series covering DevOps transformation insights from the Accelerate book.</description>
    <link>{{ site.url }}{{ site.baseurl }}</link>
    <atom:link href="{{ site.url }}{{ site.baseurl }}/videos.xml" rel="self" type="application/rss+xml" />
    <language>en-us</language>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll {{ jekyll.version }}</generator>
    <managingEditor>{{ site.email | default: "contact@example.com" }} ({{ site.author | default: "Accelerate DevEx Book Club" }})</managingEditor>
    <webMaster>{{ site.email | default: "contact@example.com" }} ({{ site.author | default: "Accelerate DevEx Book Club" }})</webMaster>
    <copyright>{{ site.time | date: "%Y" }} {{ site.author | default: "Accelerate DevEx Book Club" }}</copyright>
    <image>
      <url>{{ site.url }}{{ site.baseurl }}/assets/images/video-logo.png</url>
      <title>{{ site.title }} Videos</title>
      <link>{{ site.url }}{{ site.baseurl }}</link>
      <width>1280</width>
      <height>720</height>
    </image>

    <!-- Meeting Video Episodes - Only Supplementary Media (No "Main" Entries) -->
    {% assign meetings = site.meetings | sort: 'week' %}
    {% for meeting in meetings %}
      <!-- Supplementary video files for this meeting -->
      {% assign meeting_key = 'meeting_' | append: meeting.week %}
      {% if site.data.supplementary_media[meeting_key] and site.data.supplementary_media[meeting_key].video %}
        {% for video in site.data.supplementary_media[meeting_key].video %}
          {% assign supp_url = 'https://accelerate-devex-book-club-notebooklm.s3.amazonaws.com/' | append: video.filename %}
          <item>
            <title>{{ video.title | xml_escape }} (Bonus)</title>
            <description>{{ video.description | xml_escape }} - From meeting {{ meeting.week }}</description>
            <link>{{ site.url }}{{ site.baseurl }}{{ meeting.url }}#supplementary</link>
            <guid isPermaLink="false">{{ site.url }}{{ site.baseurl }}{{ meeting.url }}#{{ video.filename }}</guid>
            <pubDate>{{ meeting.date | default: site.time | date_to_rfc822 }}</pubDate>
            <author>{{ site.email | default: "contact@example.com" }} ({{ site.author | default: "Accelerate DevEx Book Club" }})</author>
            <enclosure url="{{ supp_url }}" type="{{ video.type }}" length="0"/>
            
            <media:content url="{{ supp_url }}" type="{{ video.type }}" medium="video" width="1920" height="1080">
              <media:title>{{ video.title | xml_escape }}</media:title>
              <media:description>{{ video.description | xml_escape }}</media:description>
              <media:keywords>devops, accelerate, bonus, supplementary, video</media:keywords>
            </media:content>
            
            <category>Technology</category>
            <category>Education</category>
            <category>DevOps</category>
            <category>Bonus Content</category>
          </item>
        {% endfor %}
      {% endif %}
    {% endfor %}
  </channel>
</rss>
